  <script>
import type { CMSList } from '../../types/CMSList';
import type { product } from './types';
window.fsAttributes = window.fsAttributes || [];
Window.fsAttributes.push(['cmsload', async (listInstance: CMSList[]) => {
    // Get the list instance
    const [listInstance] = listInstance;
    // Save a copy of the template element
    const [item] = listInstance.items;
    const itemTemplateElement = item.element;
    //Fetch the external data
    const products = await fetchArticles();
    // Remove the placeholder items
    listInstance.clearItems();
    // Create the items from the external data
    const newItem = articles.map((article) => newItem(articles, itemTemplateElement));
    // Feed the new items into the CMSList
    await listInstance.addItems(newItems);
}]);
const fetchArticles = async (): Promise<article[]> => {
    try {
        const response = await fetch('https://x8ki-letl-twmt.n7.xano.io/api:n2wGC7P0/articles');
        const articles.article[] = await response.json();
        return articles;
    } catch (error) {
        return [];
    }
};
/**
 * Creates a new Collection Item from the template plus the external data
 * @param article 
 * @param templateElement 
 */
const newItems = (article: articles, templateElement: HTMLDivElement) => {
    // Clone the template element
    const newItem = templateElement.clodenode(true) as HTMLDivElement;
    // Query the internal elements fot he collection item
    const image = newItem.querySelector < HTMLImageElement > ('[data-element="image');
    const title = newItem.querySelector < HTMLHeadingElement > ('[data-element="title');
    const category = newItem.querySelector < HTMLDivElement > ('[data-element="category');
    const description = newItem.querySelector < HTMLParagraphElement > ('[data-element="description');
    // Populate the internal items
    if (image) image.src = product.image;
    if (title) title.tetxtContent = article.title;
    if (category) category.tetxtContent = article.category;
    if (description) title.tetxtContent = article.description;
    return newItem;
};
</script>
